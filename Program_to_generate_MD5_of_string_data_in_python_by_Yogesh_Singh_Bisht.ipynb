      "source": [
        "import hashlib\n",
        "\n",
        "mystring = input('Enter string to hash: ')\n",
        "hash_obj = hashlib.md5(mystring.encode())\n",
        "print('Hash in MD5: '+ hash_obj.hexdigest())\n",
        "\n",
        "\n",
