        "import hashlib\n",
        "\n",
        "mystring = input('Enter string to hash: ')\n",
        "salt = input('Add salt: ') \n",
        "salted_string= mystring + salt\n",
        "print('salted hash is: ') \n",
        "hashlib.md5(salted_string.encode()).hexdigest()"
